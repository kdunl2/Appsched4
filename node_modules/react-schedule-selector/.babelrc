{
  "presets": [
    "react",
    "flow",
    [
      "env",
      {
        "targets": {
          "browsers": "last 2 versions"
        },
        "loose": true
      }
    ]
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    ["babel-plugin-styled-components", {
      "fileName": "false"
    }]
  ]
}